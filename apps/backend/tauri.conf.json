{
  "productName": "Chromatic",
  "version": "0.1.0",
  "identifier": "Chromatic",
  "build": {
    "beforeDevCommand": "pnpm vite dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm vite build",
    "frontendDist": "../../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Chromatic",
        "width": 1280,
        "minWidth": 1100,
        "height": 800,
        "minHeight": 800,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
