{
  "packages": {
    ".": {
      "skip-github-release": true,
      "release-type": "node",
      "component": "chromatic-poe",
      "include-component-in-tag": true,
      "extra-files": [
        {
          "type": "toml",
          "path": "app/tauri/Cargo.toml",
          "jsonpath": "$.package.version"
        },
        {
          "type": "json",
          "path": "app/tauri/tauri.conf.json",
          "jsonpath": "$.version"
        }
      ]
    }
  }
}
